{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchSourceUrlPrefixes": ["https://github.com/tunnelvisionlabs/antlr4ts"],
      "allowedVersions": "!/(-dev|-\\w+\\.[a-z0-9]{8})$/"
    },
    {
      "matchPackageNames": ["node", "amd64/node"],
      "versioning": "node"
    }
  ]
}
